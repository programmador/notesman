{% extends "layout.twig" %}

{% block title %}New random user creation{% endblock %}

{% block content %}

    <table class="table table-striped">
        <tbody>
            <tr>
                <td>
                    id:
                </td>
                <td>
                    {{ user.id }}
                </td>
            </tr>
            <tr>
                <td>
                    email:
                </td>
                <td>
                    {{ user.email }}
                </td>
            </tr>
            <tr>
                <td>
                    name:
                </td>
                <td>
                    {{ user.name }}
                </td>
            </tr>
        </tbody>
    </table>

{% endblock %}